<h3>Create Event</h3>

<%= form_with model: @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  
  <%= f.label :event_date %>
  <%= f.text_field :event_date %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Create Event" %>

  <%= @user.username %>
<%end%>
